    <!-- http://iosb.fraunhofer.de/ICS-Security#AllowedConnection -->

    <owl:Class rdf:about="http://iosb.fraunhofer.de/ICS-Security#AllowedConnection">
        <rdfs:subClassOf rdf:resource="http://iosb.fraunhofer.de/ICS-Security#Configuration"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#pfDestInterface"/>
                <owl:someValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#IpV4Interface"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#pfDestNetwork"/>
                <owl:someValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#Network"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#pfSourceInterface"/>
                <owl:someValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#IpV4Interface"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
