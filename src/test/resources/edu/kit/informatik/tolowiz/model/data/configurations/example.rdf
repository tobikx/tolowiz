                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#pfSourceNetwork"/>
                <owl:someValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#Network"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#destinationPort"/>
                <owl:allValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#PortRange"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://iosb.fraunhofer.de/ICS-Security#sourcePort"/>
                <owl:allValuesFrom rdf:resource="http://iosb.fraunhofer.de/ICS-Security#PortRange"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


